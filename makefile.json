{
  "defaultProject": "Test",
  "projects": [
	{
	  "name": "Test",
	  "outputPath": "bin/",
	  "optimization": "small",
	  "sources": [
		"Test/Source/*.cpp"
	  ],
	  "includes": [
		"Test/LibHeaders"
	  ],
	  "libs": [
		"bin/lib/lib"
	  ],
	  "dependencies": [ "lib" ]
	},
	{
	  "name": "lib",
	  "type": "dynamic_lib",
	  "outputPath": "bin/lib",
	  "sources": [
		"Test/LibSource/*.cpp"
	  ]
	}
  ]
}